<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="college.performance.dao.MyPerformanceDetailMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="college.performance.model.MyPerformanceDetail">
        <id column="id" property="id" />
        <result column="my_performance_id" property="myPerformanceId" />
        <result column="target_name" property="targetName" />
        <result column="metrics" property="metrics" />
        <result column="weight" property="weight" />
        <result column="score" property="score"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, my_performance_id, target_name, metrics, weight, score
    </sql>

</mapper>
